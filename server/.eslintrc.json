{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-global-assign": "off",
        "no-unused-vars": "off",
        "no-shadow": "off",
        "consistent-return": "off",
        "camelcase": "warn",
        "comma-dangle": "off",
        "no-use-before-define": "off",
        "prettier/prettier": [
            "error",
            {
              "trailingComma": "es5",
              "singleQuote": true,
              "printWidth": 100
            }
          ]
    }
}
