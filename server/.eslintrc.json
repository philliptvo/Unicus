{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-global-assign": "off",
        "no-unused-vars": "off",
        "camelcase": "warn",
        "comma-dangle": "off",
        "no-use-before-define": "off"
    }
}
